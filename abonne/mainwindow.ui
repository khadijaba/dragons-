   <string notr="true">/* QTabWidget style */
QTabWidget {
    background-color: #87CEEB;    /* Light blue background */
    color: #333333;               /* Dark gray text color */
    border: 1px solid #d0d0d0;    /* Light gray border */
    border-radius: 5px;           /* Rounded corners */
}

/* QTabBar style */
QTabBar::tab {
    background-color: #00BFFF;    /* Light blue background color for tabs */
    color: white;                 /* White text color */
    border: 2px solid #00BFFF;    /* Light blue border */
    border-top-left-radius: 5px;   /* Rounded corners for the top-left tab */
    border-top-right-radius: 5px;  /* Rounded corners for the top-right tab */
    padding: 5px 10px;            /* Padding */
}

QTabBar::tab:selected {
    background-color: #1E90FF;    /* Darker light blue for the selected tab */
}

/* QTabWidget content area style */
QTabWidget::pane {
    border: 1px solid #d0d0d0;    /* Light gray border for the content area */
    border-radius: 5px;           /* Rounded corners */
    background-color: #ffffff;     /* White background color */
}

/* QTabWidget content area when tab is selected style */
QTabWidget::tab:selected::pane {
    background-color: #f0f0f0;    /* Light gray background for the selected tab content */
}
