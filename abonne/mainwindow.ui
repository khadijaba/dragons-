/* Header style */
QHeaderView::section {
    background-color: #3fb5d0;      /* Light blue background color for header */
    color: white;                   /* White text color */
    padding: 5px;                   /* Padding for header */
    border: 1px solid #3fb5d0;      /* Light blue border for header */
}

/* Horizontal header style */
QHeaderView::section:horizontal {
    border-top-left-radius: 5px;    /* Rounded corners for the top-left header */
    border-top-right-radius: 5px;   /* Rounded corners for the top-right header */
}

/* Vertical header style */
QHeaderView::section:vertical {
    border-bottom-left-radius: 5px; /* Rounded corners for the bottom-left header */
    border-top-left-radius: 5px;    /* Rounded corners for the top-left header */
}

/* Cell style */
QTableWidget::item {
    padding: 5px;                   /* Padding for cell */
    border: 1px solid #b5d0e4;      /* Light blue border for cell */
}

/* Selected cell style */
QTableWidget::item:selected {
    background-color: #329bb0;      /* Slightly darker blue background color for selected cell */
    color: white;                   /* White text color for selected cell */
}
