/* Combo Box style */
QComboBox {
    background-color: #ecf4f9;      /* Light blue background color */
    color: #333333;                 /* Dark gray text color */
    border: 1px solid #b5d0e4;      /* Light blue border */
    border-radius: 5px;             /* Rounded corners */
    padding: 3px;                   /* Padding */
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;                    /* Width of the dropdown arrow */
    border-left-width: 1px;         /* Thickness of the border */
    border-left-color: #b5d0e4;     /* Color of the border */
    border-left-style: solid;
    background-color: transparent;  /* Transparent background */
    border-top-right-radius: 5px;   /* Rounded top-right corner */
    border-bottom-right-radius: 5px;/* Rounded bottom-right corner */
}

QComboBox::down-arrow {
    image: url(down-arrow.png);     /* Replace 'down-arrow.png' with your arrow image */
}

