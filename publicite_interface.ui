       <property name="styleSheet">
        <string notr="true">/* Bouton normal couleur noire */
QPushButton {
    border: 2px solid #000000; /* Bordure noire */
    border-radius: 15px; /* Coins arrondis */
    padding: 8px 16px; /* Espacement interne */
    background-color: rgba(0, 0, 0, 0.8); /* Fond noir avec plus de transparence */
    color: #ffffff; /* Couleur du texte en blanc */
    font-size: 24px; /* Taille de la police, ajustez selon vos préférences */
    transition: border-radius 0.3s ease-in-out, padding 0.3s ease-in-out, background-color 0.3s ease-in-out, color 0.3s ease-in-out, font-size 0.3s ease-in-out; /* Ajout de la transition pour différentes propriétés */
}

QPushButton:hover {
    /* Propriétés au survol */
    padding: 10px 20px; /* Agrandir le padding au survol */
    background-color: rgba(0, 0, 0, 1); /* Fond noir plus intense au survol */
    font-size: 18px; /* Taille de la police plus grande au survol */
}

QPushButton:pressed {
    /* Propriétés lorsqu'appuyé */
    background-color: rgba(0, 0, 0, 0.6); /* Fond noir plus foncé lorsqu'appuyé */
}

QPushButton:disabled {
    /* Propriétés lorsque désactivé */
    border: 2px solid #566573; /* Bordure gris foncé lorsqu'il est désactivé */
    color: #bdc3c7; /* Couleur du texte en gris foncé lorsque désactivé */
    background-color: rgba(0, 0, 0, 0.4); /* Fond noir avec plus de transparence lorsqu'il est désactivé */
}

QPushButton:focus {
    /* Propriétés avec le focus */
    border: 2px solid #000000; /* Bordure noire plus foncée pendant le focus */
    outline: none; /* Supprimer l'effet de soulignement bleu sur le focus */
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.7); /* Légère ombre lorsqu'en focus */
}
</string>
